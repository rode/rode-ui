{
  "name": "rode-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fmt": "prettier --write .",
    "lint": "eslint --max-warnings=0 .",
    "start": "next start",
    "test": "jest",
    "verify": "prettier --check . && yarn lint && yarn test"
  },
  "dependencies": {
    "http-status-codes": "^2.1.4",
    "next": "10.0.6",
    "node-fetch": "^2.6.1",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.32.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.1",
    "chance": "^1.1.7",
    "eslint": "^7.20.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-react": "^7.22.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ]
  }
}
